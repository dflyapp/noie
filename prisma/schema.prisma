generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model todo {
  id   String  @id
  task String
  done Boolean @default(false)
}

model images {
  id        String   @id
  createdAt DateTime @default(now())
  name      String?
}

model blogs {
  id          String   @id
  createdAt   DateTime @default(now())
  title       String
  slug        String
  description String
  content     Json
}

model animal {
  id   String @id
  name String
}
