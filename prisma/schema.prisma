generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model todo {
  id   String  @id
  task String
  done Boolean @default(false)
}

model images {
  id        String   @id @default("gen_random_uuid()")
  createdAt DateTime @default(now())
  name      String?
}
